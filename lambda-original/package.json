{
  "name": "migrate-dynamodb-pictures",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "24"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node24 --outfile=dist/index.js --format=cjs --alias:@/infinity=../../backend/src --external:pg-query-stream --external:mysql --external:mysql2 --external:better-sqlite3 --external:sqlite3 --external:tedious --external:oracledb",
    "migrate": "tsx src/cli.ts run",
    "migrate:start": "tsx src/cli.ts start",
    "migrate:stop": "tsx src/cli.ts stop",
    "migrate:status": "tsx src/cli.ts status",
    "migrate:logs": "tsx src/cli.ts logs -f",
    "migrate:reset": "tsx src/cli.ts reset",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.4.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.433.0",
    "dotenv": "^16.3.1",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
